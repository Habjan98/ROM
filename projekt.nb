    InterpretationFunction->(RowBox[{"Limit", "[", 
       RowBox[{#, ",", 
         RowBox[{#2, "->", #3}]}], "]"}]& )], TraditionalForm]],
  CellChangeTimes->{{3.7884420894559813`*^9, 3.788442104560004*^9}, 
    3.78844213492204*^9, 3.788447898304625*^9},ExpressionUUID->
  "7ab4a765-dc3b-48a3-99b8-0cc99fbbe996"],
 "= 1\n\nOd prej vemo, da za vsak n \[Element] \[DoubleStruckCapitalN] velja \
ocena ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", " ", "+", " ", 
      RowBox[{"1", "/", "n"}]}], ")"}], "n"], TraditionalForm]],
  ExpressionUUID->"a4e45ccd-7141-409f-972d-a695db8f5268"],
